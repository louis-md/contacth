<td class="table-division">
  <div><img src="{{this.avatar}}" alt="img" class="table-image" /></div>
</td>
<td class="table-division">
  <div>{{this.lastName}}</div>
</td>
<td class="table-division">
  <div>
    <ul>
      {{#each this.phoneNumbers}}
      <li>
        {{this}}
      </li>
      {{/each}}
    </ul>
    {{!-- {{this.phoneNumbers}} --}}
    </div>
</td>

<td class="table-division">
  <a href="/products/contact-edit/{{this._id}}" data-id-sneaker="{{this._id}}" class="fa fa-edit table-icon"></a>
</td>
<td class="table-division">
  <span class="fa fa-trash table-icon" data-id-sneaker="{{this._id}}"  aria-hidden="true"></span>
</td>